{
  "manifest_version": 2,
  "name": "Madeleine",
  "description": "A simple chrome application",
  "version": "0.1",
  "permissions": [
  ],
  "content_security_policy": "script-src 'self' chrome-extension-resource: 'unsafe-eval' https://*.google.com; object-src 'self'",
  "icons": {
    "128": "img/icon_128.png"
  },
  "app": {
    "launch": {
      "local_path": "index.html",
      "container": "tab"
    }
  }
}

